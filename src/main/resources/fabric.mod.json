{
	"schemaVersion": 1,
	"id": "modad",
	"version": "${version}",
	"name": "Mod of Doom and Despair",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/modad/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.bubbaboogs.modad.ModOfDoomAndDespair"
		],
		"client": [
			"com.bubbaboogs.modad.ModOfDoomAndDespairClient"
		],
		"fabric-datagen": [
			"com.bubbaboogs.modad.ModOfDoomAndDespairDataGenerator"
		],
		"cardinal-components": [
			"com.bubbaboogs.modad.components.ModCardinalComponents"
		]
	},
	"mixins": [
		"modad.mixins.json",
		{
			"config": "modad.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.9",
		"java": ">=21",
		"fabric-api": "*",
		"cardinal-components-entity": ">=7.1.0-beta.1"
	},
	"custom": {
		"cardinal-components": [
			"modad:life_crystals_used"
		]
	}
}